<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="author" content="smarquina">
    <link rel="icon" type="image/png" href="{{ asset('img/logos/favicon.png') }}">

    {% block header %}{% endblock header %}
    {% block stylesheets %}{% endblock stylesheets %}
    <!-- Bootstrap Core CSS -->
    <link href="{{ asset('css/bootstrap.min.css') }}" rel="stylesheet" type="text/css">
    <!-- JQuery extensions CSS -->
    <link href="{{ asset('css/jquery/jquery.dropdown.min.css') }}" rel="stylesheet" type="text/css">

    <!-- Custom CSS -->
    <link href="{{ asset('css/custom.css') }}" rel="stylesheet" type="text/css">

    <!-- Fonts -->
    <link href="{{ asset('/css/font-awesome.min.css') }}" rel="stylesheet" type="text/css">
    <link href="//fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet" type="text/css"/>

</head>

<body id="page-top" data-spy="scroll" data-target=".navbar-custom">

{% block navbar %}{% endblock navbar %}

{% block body %}{% endblock body %}

{% block footer %}{% endblock footer %}

<!-- Core JavaScript Files -->

<script src="{{ asset('js/jquery/jquery.min.js') }}"></script>
<script src="{{ asset('js/jquery/jquery.dropdown.min.js') }}"></script>

<script src="{{ asset('js/bootstrap.min.js') }}"></script>

<script src="{{ asset('js/custom.js') }}"></script>

{% block javascript %}{% endblock javascript %}
</body>
</html>